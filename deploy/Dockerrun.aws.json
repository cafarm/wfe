{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "<AWS_ACCOUNT_ID>.dkr.ecr.us-west-2.amazonaws.com/<NAME>:<TAG>",
    "Update": "true"
  },
  "Ports": [
    {
      "ContainerPort": "443"
    }
  ],
  "Volumes": [
    {
      "HostDirectory": "/etc/pki/tls/certs",
      "ContainerDirectory": "/etc/pki/tls/certs"
    }
  ]
}